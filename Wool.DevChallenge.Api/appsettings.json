{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning",
        "Microsoft.AspNetCore.Authentication": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3} {CorrelationId}] {Message:lj} {NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "Application": "wlx-dev-challenge.api"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "Url": "http://dev-wooliesx-recruitment.azurewebsites.net",
    "Token": "8f7dbab1-55d0-4ac1-bb6f-d89896358e26",
    "Name": "Waseem Anis"
  }
}
